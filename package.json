    "cli": "^1.0.1",
    "concurrently": "^5.3.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^6.1.0",
    "mongoose": "^5.10.9",
    "multer": "^1.4.2",
    "multer-s3": "^2.10.0",
